    function GetSearchResults() {
        //var rbPaymentmode = $("input[name='rbPayment']:checked").val();
        //var searchTerm = $("#txtSearchTerm").val();
        var searchTerm = $("#txtSearch").val();
        //console.log("SearchTerm:" + searchTerm);
