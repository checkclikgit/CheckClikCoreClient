                            $("#txtEmailMobile").val('');
                            $("#txtOTP").val('');
                            $("#txtPassword").val('');
                            $("#txtConfirmPassword").val('');
                        }
                        else {
                            if (res.Message = "Invalid OTP / OTP Expired") {
                                errMessage = res.Message + ', Please use resend link to get new OTP <u><a href="#" onclick="return lnkSentOTP()">Re-Send OTP</a></u>';
                                $("#pwdResendorinvalidOTP").html(errMessage);
                            }
                            else {
                                $("#pwdResendorinvalidOTP").html(errMessage);
                            }

                        }
                    }
                });
            }
            else {
                //show error message for confirm password

                switch (failcode) {
                    case 1:
                        errMessage = "All fields are required.";
                        break;
                    case 2:
                        errMessage = "Confirm Password doesn't match, please enter same password.";
                        break;
                }
                $("#pwdValidationMessage").html(errMessage);
            }


        });
    </script>
</body>
