        function changeUserType(type) {
            var _loginText = $("#txtLoginText");
            $(_loginText).val('');
            if (type == 1) {
                $(_loginText).attr("placeholder", "Email or Phone Number")
                $(_loginText).attr('onkeypress', '');
                $("#spnForgotType").text("Vendor");
                $("#txtLoginText").addClass('clsLoginText');
                $(_loginText).attr('onkeyup', 'return LoginTextKeyUp(event);');
                $(_loginText).attr('onkeydown', 'return LoginTextKeyDown(event);');
            }
            else if (type == 2) {
                $("#txtLoginText").removeClass('clsLoginText');
                $(_loginText).attr("placeholder", "Access ID");
                $("#txtLoginText").attr('onkeypress', 'return isNumberKey(event);');
                $("#txtLoginText").attr('onkeyup', '');
                $("#txtLoginText").attr('onkeydown', '');
                $("#spnForgotType").text("Access");
                $("#lgnPhone").hide();
                $("#lgnEmail").hide();
            }
        }
        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if ((charCode < 47 || charCode > 57))
                return false;
            return true;
        } 
        function validateData(newData) {
            var foundNaN = false;
            if (newData.length <= 9) {
                //expecting it to be a mobile number
                var partly = newData.split('');
                var i = 0;
                for (i; i < partly.length; i++) {
                    var x = parseInt(partly[i], 10);
                    if (isNaN(x))
                        foundNaN = true;
                }
                if (!foundNaN)
                    return 2
            }
            return 4;
        }

        function ValidateEmail(email) {
            var expr = /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            return expr.test(email);
        }
        function ValidateMobile(mobile) {
            if (mobile.length == 9)
                return true;
            return false;
        }
