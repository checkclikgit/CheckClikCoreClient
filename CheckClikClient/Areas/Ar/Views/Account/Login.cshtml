    <script>

        $("#btnSendOTP").click(function () {
            $("#pb_loader").show();
            var ipType = $("#htnIpType").val();
            var ipContent = (ipType === "2" ? "+966" : "") + $("#txtEmailMobile").val();
            var data = {
                "MobileNo": ipContent,
                "FlagId": 1
            };

            var isSuccessData = false;
            //if (ipType === "2") {
            //    isSuccessData = ValidateMobile($("#txtEmailMobile").val());
            //    $("#faPhone").show();
            //    $("#faEmail").hide();
            //}
            //else {
            isSuccessData = ValidateEmail($("#txtEmailMobile").val());
            //$("#faPhone").hide();
            $("#faEmail").show();
            //}
            var validationMessage = "";
            if (!isSuccessData) {
                if (ipType === "1") validationMessage = "Invalid email";
                else
                    validationMessage = "Email is Required";
                $("#pb_loader").hide();
            }
            else {
                //step 1: Send OTP using Ajax call and on success of ajax call start step 2
                //step 2: show Verify OTP Screen with new passwords fields
                console.log(data);
                $("#pwdOTPSent").html("");
                var res = { Status: false, Message: "default" };
                $("#ValidationMessage").text("OTP Sending Please Wait ...");
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("SentForgotPasswordOTP")',
                    data: { "jsondata": JSON.stringify(data) },
                    //contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        res.Status = data.Status;
                        res.Message = data.Message;
                        //$("#txtId").val(data.Id);
                    },
                    error: function (data) {
                        console.log("Response Text:" + data.responseText + "\n StatusText" + data.StatusText);
                        $("#pb_loader").hide();
                        $("#ValidationMessage").text("");
                        res = false;
                    },
                    complete: function () {
                        $("#pb_loader").hide();
                        $("#ValidationMessage").text("");
                        if (res.Status) {
                            $("#forgot__password").modal("hide");
                            $("#VerifyOTP__password").modal("show");
                        }
                        else {
                            validationMessage = res.Message;
                            $("#ValidationMessage").html(validationMessage);
                        }
                    }
                });
            }
            $("#ValidationMessage").html(validationMessage);
            //alert(JSON.stringify(data));

        });
        $('#txtOTP').keyup(function (e) {
            if (/\D/g.test(this.value)) {
                // Filter non-digits from input value.
                this.value = this.value.replace(/\D/g, '');
            }
        });

