                                    <input type="submit" class="btn btn-default btn-block btn-lg search-button" value="Search">
                                </div>
                                <input type="hidden" name="hdnlocation" id="hdnlocation" /> 
                                <input type="hidden" name="hdnIsLocationSet" id="hdnIsLocationSet" value="0" /> 
