                                                    {
                                                        if (notifications.Select(x => x.Unreadcount).First() > 0)
                                                        {
                                                        <sup class="badge"> @notifications.Select(x => x.Unreadcount).First()</sup>

                                                        }
                                                    }
                                            </a>
                                            <ul class="dropdown-menu notify-drop">
                                                <div class="notify-drop-title">
                                                    <div class="row">
                                                        <div class="col-md-9 col-sm-9 col-xs-9">Notifications (<b> @(notifications != null && notifications.Count() > 0 ? notifications.Select(x => x.Unreadcount).First() : 0)</b>)</div>
                                                        <div class="col-md-6 col-sm-6 col-xs-6 text-right"><a href="" class="rIcon allRead" data-tooltip="tooltip" data-placement="bottom"></a></div>
                                                    </div>
                                                </div>

                                                    @if (notifications != null && notifications.Count > 0)
                                                    {
                                                    <div class="drop-content">
                                                            @foreach (var item in notifications)
                                                            {
                                                        <li>
                                                                    @if (item.OrderId != 0)
                                                                    {
                                                                <a href="@Url.Action("index", "myorders", new { Id = item.OrderId })">
                                                                    <div class="col-md-3 col-sm-3 hidden-xs"><div class="notify-img"></div></div>
                                                                    <div class="col-md-9 col-sm-9 col-xs-12 pd-l0">
                                                                                @item.MessageEn
                                                                    </div>
                                                                </a>
                                                                    }
                                                                    else
                                                                    {
                                                                <div class="col-md-3 col-sm-3 hidden-xs"><div class="notify-img"><img src="https://placehold.it/45x45" alt=""></div></div>
                                                                <div class="col-md-9 col-sm-9 col-xs-12 pd-l0">
                                                                            @item.MessageEn
                                                                </div>
                                                                    }
                                                        </li>
                                                            }
                                    </div>
                                                    }

                                <div class="notify-drop-footer text-center">
                                    <a href="@Url.Action("notifications", "user")"><i class="fa fa-eye"></i> View All</a>
                                </div>
                                </ul>
                                </li>
                                        }
                                    }





                        </li>
                        <li>
                            <a href="javascript:;" onclick="openNav()" title="Cart">
