                        </li>
                        <li>
                            <a href="@ViewBag.CurrentURL">
                                <strong class="hidden-xs font16">العربية</strong>
                            </a>
                        </li>
                        </ul>
                    </div>
                    <!-- end col -->
                </div>
                <!-- end  row -->
