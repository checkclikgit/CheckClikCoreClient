                if (a >= 1) {
                    a--;
                }
                var fnl = a * parseFloat(price);

                $('#txtvalL1_' + id).val(a);

                $.ajax({
                    async: false,
                    type: "post",
                    url: '@Url.Action("UpdateProductStatus", "Cart")',
                    datatype: "json",
                    data: { CartId: id, quantity: a, flagid: 1 },
                    traditional: true,
                    success: function (data) {
                        var v = data.success;
                        if (v == true) {
