                                                @{
                                                    var cityList = new SelectList(AppUtils.CityList, "Id", "NameEn");
                                                    var districtList = new SelectList(AppUtils.DistrictList, "Id", "NameEn");
                                                }

                                                @Html.DropDownList("Id", (IEnumerable<SelectListItem>)cityList,
                                                new { @id = "select-city", @class = "select form-control", @data_live_search = "true" })

                                            </div>
