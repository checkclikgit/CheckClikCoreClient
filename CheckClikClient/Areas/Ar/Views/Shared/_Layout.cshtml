                                    }



                                    @*</li>*@

                                    @{
                                        var notifications = (List<Customer.Models.NotificationDTO>)ViewData["Nofications"];
                                    }

                                    @if (CheckClickClient.AppUtils.UserLogin != null)
                                    {
                                        //var data = (Customer.Models.SessionDTO)Session["UserLogin"];
                                        if (data.IsLogin == true)
                                        {
                                        <li>
                                            <a href="javascript:;" class="rborder dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                                <i class="far fa-bell"></i>
